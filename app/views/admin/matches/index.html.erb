<%= link_to "Back to Admin Page", admin_path, class: "btn btn-default" %>
<h1>All Matches</h1>
<table>
  <thead>
    <tr>
      <th>Day</th>
      <th>Student 1</th>
      <th>Student 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @matches.each do |match| %>
    <tr>
      <td><%= match.date %></td>
      <td><%= match.student_1_id %></td>
      <td><%= match.student_2_id %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Create matches", class: "btn btn-default" %>
